<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.mhc</groupId>
	<artifactId>chevrolet</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>

	<name>spring-boot-chevrolet</name>
	<description>chevrolet in spring boot</description>

	<modules>
		<module>chevrolet-api</module>
		<module>chevrolet-core</module>
		<module>chevrolet-dal</module>
		<module>chevrolet-web</module>
		<!--<module>chevrolet-mybatisgen</module>-->
		<!--<module>spring-boot-starter-chevrolet</module>-->
		<!--<module>chevrolet-demo-client</module>-->
	</modules>

	<properties>
		<java_source_version>1.8</java_source_version>
		<java_target_version>1.8</java_target_version>
		<file_encoding>UTF-8</file_encoding>
		<test4j.version>2.0.6</test4j.version>
		<jetty-version>8.1.13.v20130916</jetty-version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>1.8</java.version>
		<spring-boot.version>1.5.2.RELEASE</spring-boot.version>
		<gson.version>2.4</gson.version>
		<api.version>1.0.1</api.version>
		<spring.version>4.2.4.RELEASE</spring.version>
		<hummer.version>1.0.33</hummer.version>
		<infiniti-api.version>1.5.3</infiniti-api.version>
		<acura.api.version>1.9.2-SNAPSHOT</acura.api.version>
		<subaru.session.version>1.5-SNAPSHOT</subaru.session.version>
		<subaru.common.version>1.0.8-SNAPSHOT</subaru.common.version>
		<subaru.snowflake.version>1.0-SNAPSHOT</subaru.snowflake.version>
		<subaru.validation.version>1.5-SNAPSHOT</subaru.validation.version>
		<subaru.cipher.version>1.0-SNAPSHOT</subaru.cipher.version>
		<koenigsegg.dubbo.version>1.0.14</koenigsegg.dubbo.version>
		<framework-support-log.version>1.2.0-SNAPSHOT</framework-support-log.version>
		<buick-api>1.1.5</buick-api>
		<docker.skip>true</docker.skip>
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.mhc.hummer</groupId>
				<artifactId>hummer-api</artifactId>
				<version>1.5.1</version>
			</dependency>
			<dependency>
				<groupId>com.mhc</groupId>
				<artifactId>chevrolet-api</artifactId>
				<version>${api.version}</version>
			</dependency>
			<dependency>
				<groupId>com.mhc</groupId>
				<artifactId>chevrolet-dal</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>com.mhc</groupId>
				<artifactId>chevrolet-core</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>com.mhc.spring.boot</groupId>
				<artifactId>dubbo-spring-boot-starter</artifactId>
				<version>1.0.1-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>com.mhc</groupId>
				<artifactId>subaru-common</artifactId>
				<version>1.0.8-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>com.infiniti</groupId>
				<artifactId>infiniti-api</artifactId>
				<version>${infiniti-api.version}</version>
			</dependency>
			<dependency>
				<groupId>com.infiniti</groupId>
				<artifactId>spring-boot-starter-infiniti</artifactId>
