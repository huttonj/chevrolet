				<groupId>com.mhc</groupId>
				<artifactId>subaru-snowflake</artifactId>
				<version>${subaru.snowflake.version}</version>
			</dependency>
			<dependency>
				<groupId>com.mhc</groupId>
				<artifactId>subaru-validation</artifactId>
				<version>${subaru.validation.version}</version>
			</dependency>
			<dependency>
				<groupId>com.mhc</groupId>
				<artifactId>subaru-cipher</artifactId>
				<version>${subaru.cipher.version}</version>
			</dependency>
			<dependency>
				<groupId>javax.ws.rs</groupId>
				<artifactId>javax.ws.rs-api</artifactId>
				<version>2.1</version>
			</dependency>
			<dependency>
				<groupId>org.glassfish.jersey.core</groupId>
				<artifactId>jersey-common</artifactId>
				<version>2.26</version>
			</dependency>
			<dependency>
				<groupId>org.glassfish.jersey.core</groupId>
