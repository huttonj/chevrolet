				<!--<artifactId>hummer-dubbo</artifactId>-->
				<!--<version>${hummer.version}</version>-->
			<!--</dependency>-->
			<!-- 讴歌的依赖 -->
			<dependency>
				<groupId>com.maihaoche.acura</groupId>
				<artifactId>acura-api</artifactId>
				<version>${acura.api.version}</version>
			</dependency>
			<!-- 科尼塞克的依赖 -->
			<dependency>
				<groupId>com.koenigsegg.api</groupId>
				<artifactId>Koenigsegg-Dubbo</artifactId>
				<version>${koenigsegg.dubbo.version}</version>
			</dependency>
			<!-- 斯巴鲁的依赖 -->
			<dependency>
				<groupId>com.mhc</groupId>
				<artifactId>subaru-session</artifactId>
				<version>${subaru.session.version}</version>
			</dependency>
			<dependency>
				<groupId>com.mhc</groupId>
				<artifactId>subaru-common</artifactId>
				<version>${subaru.common.version}</version>
				<exclusions>
					<exclusion>
						<groupId>org.springframework</groupId>
